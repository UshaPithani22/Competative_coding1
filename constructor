//Program for constructors overloading
class Student
{
  String name;
  int age;
  Student()
  {
    name = "UnKnown";
    age = 0;
  }
  Student(String n, int a)
  {
    name = n;
    age = a;
   }
   void display()
   {
     System.out.println("Name: " + name + ", Age: " + age);
   }
}
class Constructor
{
  public static void main(String args[])
  {
    Student s1 = new Student();
    s1.display();
    Student s2 = new Student("Usha", 21);
    s2.display();
 }
 }
